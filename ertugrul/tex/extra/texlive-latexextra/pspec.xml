            <AdditionalFile owner="root" permission="0755" target="/usr/bin/ppower4">ppower4</AdditionalFile>
        </AdditionalFiles>
        <Provides>
            <COMAR script="package.py">System.Package</COMAR>
        </Provides>
    </Package>

